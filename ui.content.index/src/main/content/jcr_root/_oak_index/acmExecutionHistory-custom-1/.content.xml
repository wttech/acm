                <id
                    jcr:primaryType="nt:unstructured"
                    name="id"
                    nodeScopeIndex="{Boolean}true"
                    propertyIndex="{Boolean}true"
                    index="{Boolean}true"
                />
                <executableId
                    jcr:primaryType="nt:unstructured"
                    name="executableId"
                    nodeScopeIndex="{Boolean}true"
                    propertyIndex="{Boolean}true"
                    index="{Boolean}true"
                />
                <status
                    jcr:primaryType="nt:unstructured"
                    name="status"
                    nodeScopeIndex="{Boolean}true"
                    propertyIndex="{Boolean}true"
                    index="{Boolean}true"
                />
                <startDate
                    jcr:primaryType="nt:unstructured"
                    name="startDate"
                    nodeScopeIndex="{Boolean}true"
                    propertyIndex="{Boolean}true"
                    index="{Boolean}true"
                />
                <endDate
                    jcr:primaryType="nt:unstructured"
                    name="endDate"
                    nodeScopeIndex="{Boolean}true"
                    propertyIndex="{Boolean}true"
                    index="{Boolean}true"
                />
                <duration
                    jcr:primaryType="nt:unstructured"
                    name="duration"
                    nodeScopeIndex="{Boolean}true"
                    propertyIndex="{Boolean}true"
                    index="{Boolean}true"
                />
                <error
                    jcr:primaryType="nt:unstructured"
                    name="error"
                    nodeScopeIndex="{Boolean}true"
                    propertyIndex="{Boolean}true"
                    index="{Boolean}true"
                />
            </properties>
        </nt:base>
    </indexRules>
</jcr:root>